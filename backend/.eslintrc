{
   "extends": [
      "airbnb-base",
      "eslint:recommended"
   ],
   "env": {
      "commonjs": true,
      "es6": true,
      "jest": true,
      "node": true
   },
   "settings": {
      "import/resolver": {
         "node": {
            "paths": [
               "."
            ],
            "extensions": [
               ".js"
            ]
         }
      }
   },
   "rules": {
      "import/prefer-default-export": 0,
      "semi": [
         1,
         "always"
      ],
      "quotes": [
         1,
         "single",
         {
            "avoidEscape": true
         }
      ],
      "indent": [
         1,
         3
      ],
      "arrow-parens": [
         1,
         "always"
      ],
      "space-in-parens": [
         1,
         "always"
      ],
      "block-spacing": [
         1,
         "always"
      ],
      "array-bracket-spacing": [
         1,
         "always"
      ],
      "object-curly-spacing": [
         1,
         "always"
      ],
      "brace-style": [
         1,
         "stroustrup"
      ],
      "lines-between-class-members": [
         1,
         "always"
      ],
      "padding-line-between-statements": [
         "error",
         {
            "blankLine": "always",
            "prev": "block-like",
            "next": "*"
         },
         {
            "blankLine": "always",
            "prev": "*",
            "next": "block-like"
         },
         {
            "blankLine": "always",
            "prev": "*",
            "next": "return"
         },
         {
            "blankLine": "always",
            "prev": "*",
            "next": "export"
         },
         {
            "blankLine": "always",
            "prev": "export",
            "next": "*"
         }
      ],
      "no-unused-vars": [
         "off"
      ],
      "func-names": [
         "off"
      ],
      "no-use-before-define": [
         "off"
      ],
      "no-console": [
         "off"
      ]
   }
}
